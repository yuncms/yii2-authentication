{
  "name": "yuncms/yii2-authentication",
  "description": "authentication",
  "keywords": [
    "authentication",
    "yii2",
    "yii2-module"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Xu TL",
      "email": "xutongle@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "xutl/yii2-bootstrap-filestyle-asset": "~1.0",
    "yuncms/yii2-user": "~2.0",
    "xutl/yii2-id98": "~1.0.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\authentication\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "authentication",
      "i18n": {
        "class":"yii\\i18n\\PhpMessageSource",
        "basePath":"@yuncms/authentication/message"
      },
      "migrationNamespace": "yuncms\\authentication\\migrations",
      "backend": {
        "class": "yuncms\\authentication\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\authentication\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
